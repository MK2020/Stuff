#ifndef FUNCTIONS2_H_
#define FUNCTIONS2_H_


typedef myFunctions (*fnc_name)(int a, int b);

struct myFunctions{

//default constructor
	myFunctions();
//overloaded contsructor
	myFunctions(int a, int b, fnc_name name);

	int val_a;
	int val_b;


};




#endif /* FUNCTIONS2_H_ */
